<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="20250120110515-1" author="marco.gomes">
        <createTable tableName="document_extractor_types">
            <column name="DOCUMENT_ID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="DOCUMENT_EXTRACTOR" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addForeignKeyConstraint constraintName="FK_DOCUMENT_EXTRACTOR"
                                 baseTableName="document_extractor_types"
                                 baseColumnNames="DOCUMENT_ID"
                                 referencedTableName="tb_document"
                                 referencedColumnNames="ID"
                                 onDelete="CASCADE"/>
    </changeSet>

</databaseChangeLog>
