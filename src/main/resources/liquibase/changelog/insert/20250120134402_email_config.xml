<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="20250120134402-1" author="marco.gomes">
        <loadData file="classpath:liquibase/changelog/insert/csv/email_search_term.csv" tableName="tb_email_search_term"
                  separator="," encoding="UTF-8" usePreparedStatements="true">
            <column name="id" type="UUID"/>
            <column name="term" type="VARCHAR"/>
        </loadData>
    </changeSet>

    <changeSet id="20250120134402-2" author="fidelis.silva">
        <insert tableName="tb_email_search_term">
            <column name="ID" value="8"/>
            <column name="TERM" value="NF"/>
        </insert>
    </changeSet>

    <changeSet id="20250120134402-3" author="fidelis.silva">
        <insert tableName="tb_email_search_term">
            <column name="ID" value="9"/>
            <column name="TERM" value="nota"/>
        </insert>
    </changeSet>

    <changeSet id="20250120134402-4" author="fidelis.silva">
        <insert tableName="tb_email_search_term">
            <column name="ID" value="10"/>
            <column name="TERM" value="recibo"/>
        </insert>
    </changeSet>
</databaseChangeLog>
